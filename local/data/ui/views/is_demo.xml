<form script="multiselect.js" stylesheet="timepicker.css" theme="dark" refresh="30">
  <label>IS DEMO</label>
  <description>This is for demo purposes</description>
  <search id="base_search">
    <query>
        source="is_test" | rename payloadData._timestamp as timestamp payloadData.serviceProvider as serviceProvider payloadData.authenticationSuccess as logged payloadData.username as usr payloadData.eventType as type payloadData.authStepSuccess as step payloadData.tenantDomain as tenant payloadData.userStoreDomain as userstore payloadData.remoteIp as IpAddress| fields *
    </query>
    <earliest>$timepicker.earliest$</earliest>
    <latest>$timepicker.latest$</latest>
  </search>
  <fieldset submitButton="false" autoRun="false">
    <input type="time" token="timepicker">
      <label>Time Picker</label>
      <default>
        <earliest>-60m@m</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input id="multi1" type="multiselect" token="serviceProvider">
      <label>ServiceProvider</label>
      <choice value="*">All</choice>
      <default>*</default>
      <fieldForLabel>serviceProvider</fieldForLabel>
      <fieldForValue>serviceProvider</fieldForValue>
      <!-- <change>-->
      <!--<eval token="form.serviceProvider">case(mvcount('form.serviceProvider')=0,"All",mvcount('form.serviceProvider')&gt;1 AND mvfind('form.serviceProvider',"All")&gt;0,"All",mvcount('form.serviceProvider')&gt;1 AND mvfind('form.serviceProvider',"All")=0,mvfilter('form.serviceProvider'!="All"),1==1,'form.serviceProvider')</eval>-->
      <!--<eval token="env">if(mvfind('form.serviceProvider',"All")=0,"serviceProvider=*",$serviceProvider$)</eval>-->
      <!--    <eval token="form.serviceProvider">if(mvcount('form.serviceProvider')=0,"*",if(mvcount('form.serviceProvider')!=1,mvfilter('form.serviceProvider'!="*"),'form.serviceProvider'))</eval>-->
      <!--</change>-->
      <search base="base_search">
        <query> search $tenant$ AND $usr$ |stats count by serviceProvider</query>
      </search>
      <initialValue>*</initialValue>
      <valuePrefix>serviceProvider="</valuePrefix>
      <valueSuffix>"</valueSuffix>
      <delimiter> OR </delimiter>
    </input>
    <input id="multi2" type="multiselect" token="tenant">
      <label>Tenant</label>
      <choice value="*">All</choice>
      <default>*</default>
      <fieldForLabel>tenant</fieldForLabel>
      <fieldForValue>tenant</fieldForValue>
      <search base="base_search">
        <query> | search $serviceProvider$ AND $usr$ |stats count by tenant</query>
      </search>
      <initialValue>*</initialValue>
      <valuePrefix>tenant="</valuePrefix>
      <valueSuffix>"</valueSuffix>
      <delimiter> OR </delimiter>
    </input>
    <input id="multi3" type="multiselect" token="usr">
      <label>User</label>
      <choice value="*">All</choice>
      <default>*</default>
      <fieldForLabel>usr</fieldForLabel>
      <fieldForValue>usr</fieldForValue>
      <search base="base_search">
        <query> | search $tenant$ AND $serviceProvider$|stats count by usr</query>
      </search>
      <initialValue>*</initialValue>
      <valuePrefix>usr="</valuePrefix>
      <valueSuffix>"</valueSuffix>
      <delimiter> OR </delimiter>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>OverAll</title>
      <single>
        <search base="base_search">
          <query> search $tenant$ AND $serviceProvider$ AND $usr$ | where NOT (type =="step" and step =="true")  |stats count</query>
        </search>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="refresh.display">progressbar</option>
        <option name="underLabel">Total Attempts</option>
        <option name="useColors">1</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <single>
        <search base="base_search">
          <query> search $tenant$ AND $serviceProvider$ AND $usr$ | where NOT (type =="step" and step =="true")  |stats distinct_count(usr)</query>
        </search>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x0E31EB","0x6db7c6","0xf7bc38","0xf58f39","0xd93f3c"]</option>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="refresh.display">progressbar</option>
        <option name="underLabel">Total Users</option>
        <option name="useColors">1</option>
      </single>
    </panel>
    <panel>
      <single>
        <search base="base_search">
          <query> search $tenant$ AND $serviceProvider$ AND $usr$|where (step =="false" AND logged=="false") |stats count</query>
        </search>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x53a051","0x0877a6","0xf8be34","0xf1813f","0xdc4e41"]</option>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="refresh.display">progressbar</option>
        <option name="underLabel">Total Failed Attempts</option>
        <option name="useColors">1</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <map>
        <search base="base_search">
          <query> search $tenant$ AND $serviceProvider$ AND $usr$|  iplocation IpAddress | geostats count by Country</query>
        </search>
        <option name="drilldown">none</option>
        <option name="mapping.type">marker</option>
      </map>
    </panel>
    <panel>
      <viz type="semicircle_donut.semicircle_donut">
        <title>User percentage by logging success</title>
        <search base="base_search">
          <query> search $tenant$ AND $serviceProvider$ AND $usr$  | where NOT (type =="step" and step =="true") |replace "true" WITH "success", "false" WITH "failed" IN logged|stats count(usr) as attempts by logged</query>
        </search>
        <option name="drilldown">all</option>
        <option name="refresh.display">progressbar</option>
        <option name="semicircle_donut.semicircle_donut.cutoutPercentage">50</option>
        <option name="semicircle_donut.semicircle_donut.legendPosition">top</option>
        <option name="semicircle_donut.semicircle_donut.type">full</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
      </viz>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Attempt based on Tenant</title>
        <search base="base_search">
          <query>search $tenant$ AND $serviceProvider$ AND $usr$| where NOT (type =="step" and step =="true") |replace "true" WITH "Success", "false" WITH "Failed" IN logged|timechart span=1day count(usr) by tenant | fields - NULL</query>
        </search>
        <option name="charting.axisTitleX.text">Time</option>
        <option name="charting.axisTitleY.text">Attempts</option>
        <option name="charting.chart">bar</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Attempt based on ServiceProvider</title>
        <search base="base_search">
          <query> search $tenant$ AND $serviceProvider$ AND $usr$| where NOT (type =="step" and step =="true") |replace "true" WITH "Success", "false" WITH "Failed" IN logged|timechart span=1day count(usr) by serviceProvider | fields - NULL</query>
        </search>
        <option name="charting.axisTitleX.text">Time</option>
        <option name="charting.axisTitleY.text">Attempt</option>
        <option name="charting.axisY.abbreviation">auto</option>
        <option name="charting.chart">column</option>
        <option name="charting.drilldown">none</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
      </chart>
    </panel>
    <panel>
      <chart>
        <title>Top 5 service Providers based on attempts</title>
        <search base="base_search">
          <query> search $tenant$ AND $serviceProvider$ AND $usr$| where NOT (type =="step" and step =="true") |replace "true" WITH "Success", "false" WITH "Failed" IN logged|stats count(usr) as amount by serviceProvider | fields - NULL | sort -amount | head 5</query>
        </search>
        <option name="charting.axisTitleY.text">Attempt</option>
        <option name="charting.chart">column</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Login patterns over time</title>
        <search base="base_search">
          <query>search $serviceProvider$ AND $usr$ AND $tenant$| where NOT (type =="step" and step =="true") |replace "true" WITH "Success", "false" WITH "Failed" IN logged|timechart span=1d count(usr) by logged | rename _time as Time | eval Time=strftime(Time, "%d-%b-%Y") | fields - NULL</query>
        </search>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.legend.placement">right</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Details</title>
        <search base="base_search">
          <query> search $serviceProvider$ AND $usr$ AND $tenant$ | eval loginTime=strftime( timestamp/1000,"%I:%M %p %d %b %Y") | where NOT (type =="step" and step =="true") |rename step as Attempt usr as UserName serviceProvider as ServiceProvider userstore as Userstore loginTime as Time|table Time Attempt UserName  IpAddress ServiceProvider Userstore </query>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
        <format type="color" field="attempt">
          <colorPalette type="minMidMax" maxColor="#006D9C" minColor="#FFFFFF"></colorPalette>
          <scale type="minMidMax"></scale>
        </format>
      </table>
    </panel>
  </row>
</form>